{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "removeComments": false,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "noLib": false
    },
    "exclude": [
        "node_modules",
        "typings",
        "typings/main.d.ts",
        "typings/main",
        "typings/browser.d.ts",
        "typings/browser"
    ]
}